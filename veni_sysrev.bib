@article{vrolijkLongitudinalLinkagesFather2020a,
  ids = {vrolijkLongitudinalLinkagesFather2020,vrolijkLongitudinalLinkagesFatherunderreview},
  title = {Longitudinal {{Linkages Between Father}} and {{Mother Autonomy Support}} and {{Adolescent Problem Behaviors}}: {{Between}}-{{Family Differences}} and {{Within}}-{{Family Effects}}},
  shorttitle = {Longitudinal {{Linkages Between Father}} and {{Mother Autonomy Support}} and {{Adolescent Problem Behaviors}}},
  author = {Vrolijk, Paula and Van Lissa, Caspar J. and Branje, Susan J. T. and Meeus, Wim H. J. and Keizer, Renske},
  date = {2020-11-01},
  journaltitle = {Journal of Youth and Adolescence},
  shortjournal = {J Youth Adolescence},
